SELECT *
FROM EMPLOYEE;


SELECT EMP_NUM, EMP_LNAME, EMP_FNAME, EMP_HIREDATE
FROM EMPLOYEE
WHERE EMP_HIREDATE >= '2000-1-1';


SELECT JOB_DESCRIPTION, JOB_SHG_HOUR
FROM JOB_DESCRIPTIONWHERE JOB_CHG_HOUR > "50.00"
ORDER BY JOB_DESCRIPTION ASC;


SELECT ASSIGN_NUM, PROJ_NUM, ASSIGN_HOURS
FROM ASSIGNMENT 
WHERE PROJ_NUM = '15' AND ASSIGN_HOURS > '2';


SELECT EMP_FNAME AS "FIRST NAME", EMP_LNAME AS "LAST NAME"
FROM EMPLOYEE
WHERE JOB_CODE = "501";




